{% extends "base.html.twig" %}

{% block title %}Ajout d'une catégorie
{% endblock %}

{% block body %}
	<h1 class="mb-3rem">Ajouter une catégorie</h1>

	{{ form_start(form) }}

	{{ form_widget(form) }}

	<button type="submit" class="btn btn-primary btn-center">
		<i class="fas fa-save"></i>
		Ajouter la catégorie
	</button>

	{{ form_end(form) }}
{% endblock %}